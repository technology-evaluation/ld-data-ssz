vocabulary schema {
  prefix "schema" "http://schema.org/"

  classes
    Organization
    PostalAdress
    CreativeWork
    DefinedTerm
    DefinedTermSet
    Place
    Dataset

  properties
    name
    identifier
    inDefinedTermSet
    termCode
    hasDefinedTerm
    isPartOf
    hasPart
    containsPlace
    containedInPlace
    description
    sameAs
    position
    validFrom
    validThrough
    disambiguatingDescription
    url
    alternateName
    startDate
    endDate
}

vocabulary dct {
  prefix "dct" "http://purl.org/dc/terms/"

  properties
    identifier
    description
    accrualPeriodicity
    issued
    modified
    publisher
    title
    license
    spatialRelationship
    language
}

vocabulary dcat {
  prefix "dcat" "http://www.w3.org/ns/dcat#"

  properties
    contactPoint
    keyword
    landingPage
    theme
}

vocabulary rdf {
  prefix "rdf" "http://www.w3.org/1999/02/22-rdf-syntax-ns#"

  classes
    Property
}

vocabulary qudt {
  prefix "qudt" "http://qudt.org/schema/qudt/"

  classes
    Unit
    NominalScale
    OrdinalScale
    IntervalScale
    RatioScale

  properties
    unit
    order
    scaleType
}

vocabulary time {
  prefix "time" "http://www.w3.org/2006/time#"

  classes
    TemporalEntity

  properties
    hasBeginning
    hasEnd
    inXSDDate
}

vocabulary ssz {
  prefix "ssz" "https://ld.stadt-zuerich.ch/schema/"

  classes
    Attribute
    LegalFoundation

  properties
    view
    dataObject
    rounding
    referenceTime
    hasBeginning
    hasEnd
    filterOf
    legalFoundation
    dataQuality
    usageNotes
    dataType
    dataUnit
    valueList
}

