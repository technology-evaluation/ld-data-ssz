language-tags {
  de
  en
  fr
  it
}

vocabulary xsd {
  prefix "xsd" "http://www.w3.org/2001/XMLSchema#"

  datatypes
    date
    float
    gYear
    integer
}

vocabulary cube {
  prefix "cube" "https://cube.link/"

  classes
    Cube
    Constraint
    ObservationSet
    Observation
    KeyDimension
    MeasureDimension
    SharedDimension

  properties
    observation
    observationConstraint
    observationSet
    observedBy
}

vocabulary meta {
  prefix "meta" "https://cube.link/meta/"

  classes
    Hierarchy

  properties
    dataKind
    applicationIgnores
    dimensionRelation
    relatesTo
    inHierarchy
    nextInHierarchy
    hierarchyRoot
}

vocabulary relation {
  prefix "relation" "https://cube.link/relation/"

  classes
    StandardError

    properties
}

vocabulary dimension {
  prefix "dimension" "https://ld.stadt-zuerich.ch/statistics/dimension/"

  properties
    ZEIT
    BEZUGZEIT
    RAUM
    ERWARTETE_AKTUALISIERUNG
    DATENSTAND
}

vocabulary property {
  prefix "property" "https://ld.stadt-zuerich.ch/statistics/property/"

  properties
    ZEIT
    TIME
    RAUM
    REPLACE
}

vocabulary skos {
  prefix "skos" "http://www.w3.org/2004/02/skos/core#"

  classes
    Concept
    ConceptScheme
    Collection

  properties
    notation
    prefLabel
    altLabel
    hiddenLabel
    member
    broader
    narrower
    inScheme
    scopeNote
}

vocabulary example {
  prefix "ex" "http://example.org/"

  properties
    ANZ_GRUPPEN
    GLOSSARID
    GRUPPENCODESORT1
    GRUPPENCODESORT2
    GRUPPENCODESORT3
    GRUPPENCODESORT4
    GRUPPENCODESORT5
    KENNZAHLKLASSE
    QUELLE
    UPDATE
    BEZUGSPERIODE
    BEZUGSZEIT
    CODE
    CODENAME
    FUSSNOTEATTRIBUT
    FUSSNOTEKENNZAHL
    FUSSNOTEKENNZAHLATTRIBUT
    FUSSNOTERAUM
    FUSSNOTEZEIT
    FUSSNOTEZELLE
    GESAMTCODE_RCS
    DIMENSION1
    DIMENSION2
    DIMENSION3
    DIMENSION4
    DIMENSION5
    KENNZAHL
    KENNZAHL_REPLACE
