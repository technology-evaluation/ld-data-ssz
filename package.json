{
  "name": "ssz-data",
  "version": "0.99.0",
  "private": true,
  "type": "module",
  "description": "Barnard pipeline with sensible defaults",
  "scripts": {
    "postinstall": "mkdirp output input",
    "fetch": "barnard59 run -v --pipeline=http://ssz.lindas.ch/pipeline/fetch pipelines/fetch.ttl --variable=SSH_HOST --variable=SSH_PORT --variable=SSH_USER --variable=SSH_PRIVATE_KEY",
    "fetchDiff": "dotenv -e .env.defaults -e .env -- ./start.sh FetchDiff",
    "output:file": "barnard59 run -v --pipeline=http://ssz.lindas.ch/pipeline/toFile pipelines/main.ttl",
    "output:store": "dotenv -e .env.defaults -e .env -- ./start.sh MainStore",
    "output:dimensions:store": "dotenv -e .env.defaults -e .env -- ./start.sh DimensionsStore",
    "output:observations:store": "dotenv -e .env.defaults -e .env -- ./start.sh ObservationsStore",
    "output:dimensions:file": "mkdir -p output && barnard59 run -v --pipeline=urn:pipeline:xrm#DimensionsFile pipelines/main.ttl",
    "cubes-to-file": "barnard59 run -v --pipeline=http://ssz.lindas.ch/pipeline/cubesToFile pipelines/main.ttl",
    "dimensions-to-file": "barnard59 run -v --pipeline=http://ssz.lindas.ch/pipeline/dimensionsToFile pipelines/main.ttl",
    "test": "stricter-standard && c8 --reporter=lcov --reporter=text mocha"
  },
  "dependencies": {
    "@rdfjs/data-model": "^2.0.1",
    "@rdfjs/term-map": "^2.0.0",
    "@tpluscode/rdf-ns-builders": "^1.1.0",
    "barnard59": "^1.1.1",
    "barnard59-base": "^1.1.0",
    "barnard59-csvw": "^1.0.0",
    "barnard59-formats": "^1.2.0",
    "barnard59-ftp": "^1.0.1",
    "barnard59-graph-store": "^1.0.1",
    "barnard59-rdf": "^1.3.0",
    "clownface": "^1.5.1",
    "csv-parse": "^5.2.2",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^5.1.0",
    "esm": "^3.2.25",
    "get-stream": "^6.0.1",
    "lodash": "^4.17.21",
    "rdf-ext": "^2.0.1",
    "rdf-stream-to-dataset-stream": "^1.0.0",
    "readable-stream": "^3.6.0",
    "unzipper": "^0.10.11",
    "url-slug": "^3.0.4"
  },
  "devDependencies": {
    "c8": "^7.11.3",
    "mkdirp": "^1.0.4",
    "mocha": "^10.0.0",
    "standard": "^17.0.0",
    "stricter-standard": "^0.2.0"
  },
  "author": "Zazuko GmbH",
  "license": "MIT",
  "bugs": {}
}
